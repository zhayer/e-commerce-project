{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "surecart/cart-icon",
  "title": "Floating Cart Icon",
  "description": "The cart icon that shows your cart quantity.",
  "version": "1.0.0",
  "category": "surecart-cart",
  "example": {},
  "attributes": {},
  "supports": {
