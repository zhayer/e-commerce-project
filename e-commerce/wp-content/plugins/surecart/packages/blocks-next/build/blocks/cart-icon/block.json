      "__experimentalDefaultControls": {
        "width": true,
        "color": true
      }
    }
  },
  "render": "file:./view.php",
  "textdomain": "surecart",
  "editorScript": "file:./index.js",
  "style": [
    "file:./style-index.css"
  ],
  "viewScriptModule": [
    "@surecart/checkout"
